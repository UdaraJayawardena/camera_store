        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:camera_store"
        mc:Ignorable="d"
        Title="userRegistration" Height="450" Width="800">
    <Grid>
        <Button x:Name="saveUser" Content="Save User" HorizontalAlignment="Left" Margin="135,264,0,0" VerticalAlignment="Top" Width="75" Click="btnSaveUser"/>
        <Button x:Name="addUser" Content="Create Login Status" HorizontalAlignment="Left" Margin="324,269,0,0" VerticalAlignment="Top" Width="183" Click="btnCreateLogin"/>
    </Grid>
</Window>
